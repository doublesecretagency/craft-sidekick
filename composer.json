{
    "name": "doublesecretagency/craft-sidekick",
    "description": "Your AI companion for rapid Craft CMS development.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "ai",
        "chatgpt",
        "openai",
        "code-generation",
        "dummy-data"
    ],
    "support": {
        "docs": "https://plugins.doublesecretagency.com/sidekick",
        "issues": "https://github.com/doublesecretagency/craft-sidekick/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Double Secret Agency",
            "homepage": "https://plugins.doublesecretagency.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0|^5.0",
        "openai-php/client": "^0.10",
        "phpdocumentor/reflection-docblock": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "doublesecretagency\\sidekick\\": "src/"
        }
    },
    "extra": {
        "name": "Sidekick",
        "handle": "sidekick",
        "class": "doublesecretagency\\sidekick\\Sidekick"
    }
}
